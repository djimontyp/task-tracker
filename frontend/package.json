  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate:api": "orval",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 40",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:strict": "eslint src --ext .ts,.tsx --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "playwright test tests/e2e/visual/",
    "test:visual:update": "playwright test tests/e2e/visual/ --update-snapshots",
    "test:a11y": "playwright test tests/e2e/a11y/ --project=chromium",
    "test:a11y:all": "playwright test tests/e2e/a11y/",
    "test:a11y:report": "playwright test tests/e2e/a11y/ --project=chromium --reporter=html",
    "generate": "plop --plopfile plopfile.mjs",
    "generate:feature": "plop --plopfile plopfile.mjs feature",
    "generate:component": "plop --plopfile plopfile.mjs component",
    "generate:hook": "plop --plopfile plopfile.mjs hook",
    "generate:page": "plop --plopfile plopfile.mjs page",
    "generate:store": "plop --plopfile plopfile.mjs store",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook",
    "test:storybook:ci": "test-storybook --ci",
    "story:check": "npx tsx scripts/check-story-coverage.ts",
    "story:check:fix": "npx tsx scripts/check-story-coverage.ts --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "bash -c 'tsc --noEmit'"
    ],
    "src/shared/**/*.tsx": [
      "npx tsx scripts/check-story-coverage.ts --staged-only"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@chromatic-com/storybook": "^4.1.3",
