# Налаштування Telegram бота
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Налаштування бази даних
DATABASE_URL=postgresql://user:password@localhost:5432/tasktracker

# Налаштування Ollama
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1

# Налаштування обробника виводу
OUTPUT_PROCESSOR_TYPE=jira
JIRA_URL=https://your-company.atlassian.net
JIRA_USERNAME=your_email@example.com
JIRA_API_TOKEN=your_jira_api_token_here

# Налаштування логування
LOG_LEVEL=INFO

# Налаштування TaskIQ
TASKIQ_BROKER_URL=redis://localhost:6379/0
TASKIQ_RESULT_BACKEND_URL=redis://localhost:6379/0
